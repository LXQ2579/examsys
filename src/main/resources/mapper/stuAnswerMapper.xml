<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.damo.examsys.dao.StuAnswerDao">

    <select id="getAnswer" resultType="stuAnswer">
        select * from t_stu_answer where stu_num = #{stuNum} and paper_id = #{paperId}
    </select>

</mapper>